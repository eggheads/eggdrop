if test "$ac_cv_cygwin" = "yes"
then
  echo $ac_n "checking for /usr/local/bind/lib/libbind.a""... $ac_c" 1>&6
echo "configure:1360: checking for /usr/local/bind/lib/libbind.a" >&5
  if test -r /usr/local/bind/lib/libbind.a
  then
